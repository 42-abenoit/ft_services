server {
    listen 5000;
	server_name _;
	root /home/www/phpmyadmin;
	index index.php;

    	location ~ \.php$ {
        	fastcgi_pass		unix:/run/php/php-fpm7.sock;
        	fastcgi_index		index.php;
			fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
			include fastcgi_params;
   			}
		}
}
